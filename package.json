{
  "name": "@acdh-ch/youbeon-frontend",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite --port 3000",
    "format": "prettier . --cache --check --ignore-path .gitignore",
    "format:fix": "npm run format -- --write",
    "generate:db": "ts-node --project ./tsconfig.node.json ./scripts/generate-db/index.ts",
    "generate:favicons": "ts-node --project ./tsconfig.node.json ./scripts/generate-favicons.ts",
    "generate:imprint": "ts-node --project ./tsconfig.node.json ./scripts/generate-imprint.ts",
    "lint": "run-p lint:*:check",
    "lint:fix": "run-p lint:*:fix",
    "lint:code:check": "eslint . --cache --ext cjs,js,mjs,ts,tsx,vue --ignore-path .gitignore",
    "lint:code:fix": "npm run lint:code:check -- --fix",
    "lint:styles:check": "stylelint \"**/*.@(css|html|vue)\" --cache --ignore-path .gitignore",
    "lint:styles:fix": "npm run lint:styles:check -- --fix",
    "prepare": "run-s generate:* setup",
    "preview": "vite preview --port 3000",
    "setup": "simple-git-hooks || exit 0",
    "test": "exit 0",
    "typecheck": "vue-tsc",
    "validate": "run-s format lint typecheck test"
  },
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "packageManager": "npm@8.11.0",
  "dependencies": {
    "@fontsource/inter": "^4.5.14",
    "@headlessui/tailwindcss": "^0.1.1",
    "@headlessui/vue": "^0.0.0-insiders.ab78fbd",
    "@heroicons/vue": "^2.0.12",
    "@stefanprobst/object": "^1.2.0",
    "@vueuse/core": "^9.3.1",
    "d3": "^7.6.1",
    "force-graph": "^1.42.13",
    "leaflet": "^1.8.0",
    "nprogress": "^0.2.0",
    "tailwindcss": "^3.2.0",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@stefanprobst/assert": "^1.0.3",
    "@stefanprobst/commitlint-config": "^1.0.3",
    "@stefanprobst/eslint-config": "^1.0.5",
    "@stefanprobst/eslint-config-vue": "^1.0.2",
    "@stefanprobst/favicons": "^2.1.1",
    "@stefanprobst/log": "^1.0.3",
    "@stefanprobst/prettier-config": "^2.0.2",
    "@stefanprobst/request": "^0.2.1",
    "@stefanprobst/stylelint-config": "^2.5.1",
    "@stefanprobst/tsconfig": "^2.2.0",
    "@types/d3": "^7.4.0",
    "@types/leaflet": "^1.8.0",
    "@types/node": "^16.11.63",
    "@types/nprogress": "^0.2.0",
    "@types/prettier": "^2.7.1",
    "@types/serialize-javascript": "^5.0.2",
    "@vitejs/plugin-vue": "^3.1.2",
    "autoprefixer": "^10.4.12",
    "dotenv": "^16.0.3",
    "eslint": "^8.25.0",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-custom-media": "^8.0.2",
    "postcss-html": "^1.5.0",
    "postcss-nesting": "^10.2.0",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "serialize-javascript": "^6.0.0",
    "serve": "^14.0.1",
    "simple-git-hooks": "^2.8.1",
    "stylelint": "^14.14.0",
    "stylelint-config-html": "^1.1.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vue-tsc": "1.0.6",
    "xlsx": "^0.18.5"
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">=0.75%",
      "not dead",
      "not op_mini all"
    ]
  },
  "commitlint": {
    "extends": "@stefanprobst/commitlint-config"
  },
  "eslintConfig": {
    "extends": [
      "@stefanprobst/eslint-config",
      "@stefanprobst/eslint-config-vue"
    ]
  },
  "lint-staged": {
    "*.@(cjs|js|mjs|ts|tsx)": [
      "eslint --cache --fix",
      "prettier --cache --write"
    ],
    "*.@(css|html)": [
      "stylelint --cache --fix",
      "prettier --cache --write"
    ],
    "*.vue": [
      "eslint --cache --fix",
      "stylelint --cache --fix",
      "prettier --cache --write"
    ],
    "*.@(json|md)": "prettier --cache --write"
  },
  "postcss": {
    "plugins": {
      "postcss-html": {},
      "postcss-custom-media": {},
      "tailwindcss/nesting": {},
      "tailwindcss": {},
      "autoprefixer": {}
    }
  },
  "prettier": "@stefanprobst/prettier-config",
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "pre-push": "npm run validate"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-html/html",
      "stylelint-config-html/vue",
      "@stefanprobst/stylelint-config"
    ]
  }
}
